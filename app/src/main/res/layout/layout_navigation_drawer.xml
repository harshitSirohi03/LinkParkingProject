<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView22"
                android:layout_width="0dp"
                android:layout_height="200dp"
                android:scaleType="fitXY"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/top_img" />

            <ImageView
                android:id="@+id/img_menuNav"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                android:layout_marginEnd="16dp"
                android:layout_marginRight="16dp"
                android:padding="5dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/menu" />

            <ImageView
                android:id="@+id/imageView24"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="150dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/user_pic" />

            <ImageView
                android:id="@+id/imageView25"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:layout_marginLeft="24dp"
                android:layout_marginTop="40dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView24"
                app:srcCompat="@drawable/home_black" />

            <com.example.linkmyparkingproject.views.SourceSansProRegularTextView
                android:id="@+id/txt_home"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp"
                android:text="Home"
                android:textColor="@android:color/black"
                android:textSize="14sp"
                android:padding="5dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView25"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/imageView25"
                app:layout_constraintTop_toTopOf="@+id/imageView25" />

            <View
                android:id="@+id/view46"
                android:layout_width="0dp"
                android:layout_height="0.5dp"
                android:layout_marginTop="8dp"
                android:background="@color/colorGreyUnderLine"
                app:layout_constraintEnd_toEndOf="@+id/txt_home"
                app:layout_constraintStart_toStartOf="@+id/txt_home"
                app:layout_constraintTop_toBottomOf="@+id/txt_home" />

            <ImageView
                android:id="@+id/imageView26"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                app:layout_constraintStart_toStartOf="@+id/imageView25"
                app:layout_constraintTop_toBottomOf="@+id/imageView25"
                app:srcCompat="@drawable/my_profile" />

            <com.example.linkmyparkingproject.views.SourceSansProRegularTextView
                android:id="@+id/txt_profile"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="My Profile"
                android:textColor="@android:color/black"
                android:textSize="14sp"
                android:padding="5dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView26"
                app:layout_constraintEnd_toEndOf="@+id/txt_home"
                app:layout_constraintStart_toStartOf="@+id/txt_home"
                app:layout_constraintTop_toTopOf="@+id/imageView26" />

            <View
                android:id="@+id/view47"
                android:layout_width="0dp"
                android:layout_height="0.5dp"
                android:layout_marginTop="8dp"
                android:background="@color/colorGreyUnderLine"
                app:layout_constraintEnd_toEndOf="@+id/txt_profile"
                app:layout_constraintStart_toStartOf="@+id/txt_profile"
                app:layout_constraintTop_toBottomOf="@+id/txt_profile" />

            <ImageView
                android:id="@+id/imageView27"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                app:layout_constraintStart_toStartOf="@+id/imageView26"
                app:layout_constraintTop_toBottomOf="@+id/imageView26"
                app:srcCompat="@drawable/identity" />

            <com.example.linkmyparkingproject.views.SourceSansProRegularTextView
                android:id="@+id/txt_identify"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="Identification"
                android:textColor="@android:color/black"
                android:textSize="14sp"
                android:padding="5dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView27"
                app:layout_constraintEnd_toEndOf="@+id/txt_profile"
                app:layout_constraintStart_toStartOf="@+id/txt_profile"
                app:layout_constraintTop_toTopOf="@+id/imageView27" />

            <View
                android:id="@+id/view48"
                android:layout_width="0dp"
                android:layout_height="0.5dp"
                android:layout_marginTop="8dp"
                android:background="@color/colorGreyUnderLine"
                app:layout_constraintEnd_toEndOf="@+id/txt_identify"
                app:layout_constraintStart_toStartOf="@+id/txt_identify"
                app:layout_constraintTop_toBottomOf="@+id/txt_identify" />

            <ImageView
                android:id="@+id/imageView28"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                app:layout_constraintEnd_toEndOf="@+id/imageView27"
                app:layout_constraintStart_toStartOf="@+id/imageView27"
                app:layout_constraintTop_toBottomOf="@+id/imageView27"
                app:srcCompat="@drawable/earning" />

            <com.example.linkmyparkingproject.views.SourceSansProRegularTextView
                android:id="@+id/txt_earning"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="Total Earnings"
                android:textColor="@android:color/black"
                android:textSize="14sp"
                android:padding="5dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView28"
                app:layout_constraintEnd_toEndOf="@+id/txt_identify"
                app:layout_constraintStart_toStartOf="@+id/txt_identify"
                app:layout_constraintTop_toTopOf="@+id/imageView28" />

            <View
                android:id="@+id/view49"
                android:layout_width="0dp"
                android:layout_height="0.5dp"
                android:layout_marginTop="8dp"
                android:background="@color/colorGreyUnderLine"
                app:layout_constraintEnd_toEndOf="@+id/txt_earning"
                app:layout_constraintStart_toStartOf="@+id/txt_earning"
                app:layout_constraintTop_toBottomOf="@+id/txt_earning" />

            <ImageView
                android:id="@+id/imageView29"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                app:layout_constraintStart_toStartOf="@+id/imageView28"
                app:layout_constraintTop_toBottomOf="@+id/imageView28"
                app:srcCompat="@drawable/admin" />

            <com.example.linkmyparkingproject.views.SourceSansProRegularTextView
                android:id="@+id/txt_adminCommission"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="Admin's Commission"
                android:textColor="@android:color/black"
                android:textSize="14sp"
                android:padding="5dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView29"
                app:layout_constraintEnd_toEndOf="@+id/txt_earning"
                app:layout_constraintStart_toStartOf="@+id/txt_earning"
                app:layout_constraintTop_toTopOf="@+id/imageView29" />

            <View
                android:id="@+id/view50"
                android:layout_width="0dp"
                android:layout_height="0.5dp"
                android:layout_marginTop="8dp"
                android:background="@color/colorGreyUnderLine"
                app:layout_constraintEnd_toEndOf="@+id/txt_adminCommission"
                app:layout_constraintStart_toStartOf="@+id/txt_adminCommission"
                app:layout_constraintTop_toBottomOf="@+id/txt_adminCommission" />

            <ImageView
                android:id="@+id/imageView30"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                app:layout_constraintStart_toStartOf="@+id/imageView29"
                app:layout_constraintTop_toBottomOf="@+id/imageView29"
                app:srcCompat="@drawable/settings" />

            <com.example.linkmyparkingproject.views.SourceSansProRegularTextView
                android:id="@+id/txt_setting"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="Settings"
                android:textColor="@android:color/black"
                android:textSize="14sp"
                android:padding="5dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView30"
                app:layout_constraintEnd_toEndOf="@+id/txt_adminCommission"
                app:layout_constraintStart_toStartOf="@+id/txt_adminCommission"
                app:layout_constraintTop_toTopOf="@+id/imageView30" />

            <View
                android:id="@+id/view51"
                android:layout_width="0dp"
                android:layout_height="0.5dp"
                android:layout_marginTop="8dp"
                android:background="@color/colorGreyUnderLine"
                app:layout_constraintEnd_toEndOf="@+id/txt_setting"
                app:layout_constraintStart_toStartOf="@+id/txt_setting"
                app:layout_constraintTop_toBottomOf="@+id/txt_setting" />

            <ImageView
                android:id="@+id/imageView31"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                app:layout_constraintStart_toStartOf="@+id/imageView30"
                app:layout_constraintTop_toBottomOf="@+id/imageView30"
                app:srcCompat="@drawable/feedback" />

            <com.example.linkmyparkingproject.views.SourceSansProRegularTextView
                android:id="@+id/txt_feedback"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="Feedback"
                android:textColor="@android:color/black"
                android:textSize="14sp"
                android:padding="5dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView31"
                app:layout_constraintEnd_toEndOf="@+id/txt_setting"
                app:layout_constraintStart_toStartOf="@+id/txt_setting"
                app:layout_constraintTop_toTopOf="@+id/imageView31" />

            <View
                android:id="@+id/view52"
                android:layout_width="0dp"
                android:layout_height="0.5dp"
                android:layout_marginTop="8dp"
                android:background="@color/colorGreyUnderLine"
                app:layout_constraintEnd_toEndOf="@+id/txt_feedback"
                app:layout_constraintStart_toStartOf="@+id/txt_feedback"
                app:layout_constraintTop_toBottomOf="@+id/txt_feedback" />

            <ImageView
                android:id="@+id/imageView32"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                app:layout_constraintStart_toStartOf="@+id/imageView31"
                app:layout_constraintTop_toBottomOf="@+id/imageView31"
                app:srcCompat="@drawable/about" />

            <com.example.linkmyparkingproject.views.SourceSansProRegularTextView
                android:id="@+id/txt_about"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="About this App"
                android:textColor="@android:color/black"
                android:textSize="14sp"
                android:padding="5dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView32"
                app:layout_constraintEnd_toEndOf="@+id/txt_feedback"
                app:layout_constraintStart_toStartOf="@+id/txt_feedback"
                app:layout_constraintTop_toTopOf="@+id/imageView32" />

            <View
                android:id="@+id/view53"
                android:layout_width="0dp"
                android:layout_height="0.5dp"
                android:layout_marginTop="8dp"
                android:background="@color/colorGreyUnderLine"
                app:layout_constraintEnd_toEndOf="@+id/txt_about"
                app:layout_constraintStart_toStartOf="@+id/txt_about"
                app:layout_constraintTop_toBottomOf="@+id/txt_about" />

            <ImageView
                android:id="@+id/imageView33"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="32dp"
                android:layout_marginBottom="100dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/imageView32"
                app:layout_constraintTop_toBottomOf="@+id/imageView32"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/logout" />

            <com.example.linkmyparkingproject.views.SourceSansProRegularTextView
                android:id="@+id/txt_logOut"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="Logout"
                android:textColor="@color/colorRed"
                android:textSize="14sp"
                android:padding="5dp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView33"
                app:layout_constraintEnd_toEndOf="@+id/txt_about"
                app:layout_constraintStart_toStartOf="@+id/txt_about"
                app:layout_constraintTop_toTopOf="@+id/imageView33" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>